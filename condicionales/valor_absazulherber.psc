Algoritmo Valor_Abs
	Escribir "*************************************";
	Escribir "* Algoritmo: calculo de valor absoluto*";
	Escribir "* Autor: Ailed Azul Herber Hernandez *";
	Escribir "*************************************";
	Escribir " ";
	definir a como real;
	definir h como real;
	Escribir "Ingresa el valor númerico"; Leer a;
	h <-  ABS(a);
	Escribir "El valor absoluto de", a," es ",h;
FinAlgoritmo
